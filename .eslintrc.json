{
  "env": {
    "browser": true,
    "commonjs": true,
    "es2021": true
  },
  "extends": [
    "plugin:react/recommended",
    "airbnb",
    "airbnb-base"
  ],
  "parserOptions": {
    "ecmaFeatures": {
      "jsx": true
    },
    "ecmaVersion": 12
  },
  "plugins": [
    "react"
  ],
  "rules": {
    "no-underscore-dangle": [
      "off",
      {
        "allow": [
          "_id"
        ]
      }
    ]
  }
}